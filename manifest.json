{
	"manifest_version": 2,
	"name":"Covid Filter",
	"version": "0.0.02",
	"content_scripts": [
		{
			"matches":[
			"https://www.youtube.com/*"	
		],
			"js": [
			"content.js"
			]
		}
	],
	"background":{
		"scripts": [
		"js/background.js"
		]
	},
	"permissions":["tabs", "storage"],
	"browser_action":{
		"default_icon": "img/covid-filter icon.png",
		"default_popup":"popup/popup.html",
		"default_title":"toggle filter on/off"
	}
}